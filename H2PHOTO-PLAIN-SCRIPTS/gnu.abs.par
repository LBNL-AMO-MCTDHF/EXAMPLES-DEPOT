#!/bin/bash

echo '

set title "Photoabsorption cross section H2 fixed nuclei 7 orbitals"

set xlabel "Energy (eV)"
set ylabel "emission/absorption\ncross section (Mb)"

set term unknown
set style data lines

set key top right

set yrange [0.1:100]

set xrange [0:60]

set logscale y

plot "Dat/ZDipoleft.Dat" using (27.2114*$1):($9) lt 7 lw 10 title "Parallel"

set term x11
replot

set out "Absorption.h2.par.eps"
set term post enhanced eps solid color 28
replot

' | gnuplot -persist


